<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yikang.real"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:name=".application.RealApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name="com.yikang.real.activity.CheckedActivity"
            android:label="@string/app_name"
            android:launchMode="standard"
            android:windowSoftInputMode="adjustPan|stateAlwaysHidden" >
        </activity>
        <activity
            android:name="com.yikang.real.activity.LoginActivity"
            android:label="@string/login_button" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yikang.real.activity.WelcomeActivity" />
        <activity android:name="com.yikang.real.activity.OldHouseDetailsActivity" />
        <activity android:name="com.yikang.real.activity.ForrentDetailsActivity" />
        <activity android:name="com.yikang.real.activity.BeseActivity" />
        <activity android:name="com.yikang.real.activity.RegisterActivity" />
        <activity android:name="com.yikang.real.activity.Result" >
        </activity>
        <activity android:name="com.yikang.real.activity.SearchActivity" >
        </activity>
        <activity android:name="com.yikang.real.activity.CityList" >
        </activity>
    </application>

</manifest>